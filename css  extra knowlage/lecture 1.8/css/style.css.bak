*
{
	margin:0px;
	padding:0px;
	box-sizing:border-box;
}
.container
{
	margin:0px auto;
	max-width:1140;	
}
.main-box
{
	margin:50px 150px ;
	width:80%;
	padding:100px;
	background-color:lightblue;
}

.float1
{
	float:left;
	width:33.33%;
	background-color:#6698FF;
	border:2px solid blue;
	border-radius:5px;
}
.float1 img
{
	background-image:url('../img/rainbow.jpg');
	background-size:cover;
	width:100%;
}
.float2
{	
	float:right;
	width:33.33%;
	background-color:#6698FF;
	border:2px solid blue;
	border-radius:5px;
	FONT-SIZE:16.9px;
}
.float2 p
{
	padding:5px;
}

.float3
{	
	PADDING:2PX;
	float:right;
	width:33.33%;
	background-color:#6698FF;
	border:2px solid BLUE;
	border-radius:5px;
	text-align:justify;
}
.float3 video
{
	width:100%;
}
.float3 h1
{
	background: rgb(233,94,48);
	background: linear-gradient(90deg, rgba(233,94,48,1) 18%, rgba(233,42,226,1) 44%, rgba(0,212,255,0.9809173669467787) 61%);
	text-align:center;
}
.clear{
	clear:both;
}
