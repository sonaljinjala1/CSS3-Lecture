*
{
	margin:0px;
	padding:0px;
	box-sizing:border-box;
}
.container
{
	max-width:1020px;
	margin:100px auto;
}
.main
{
	width:100%;
	height:600px;
	background-color:#a9a9a9;
	padding:50px;
}
.box:first-child
{
	width:30%;
	height:60%;
	background-color:#25383c;
	border-radius:20px 20px 0px 0px;
	margin:120px 0px 0px 340px;
	position:relative;
}
.box:nth-child(2)
{
	width:30%;
	height:15%;
	background-color:#c71585;
	border-radius:0px 0px 20px 20px;
	margin:-10px 0px 100px 340px;

}
.icon
{ 
	width:100px;
	height:100px;
	position:absolute;
	text-align:center;
	background-color:white;
	border-radius:100px;
	border:6px solid #c71585;
	top:-15%;
	transform:translatex(90%);
	overflow:hidden;
}
.icon i
{
	margin:8px 0px 0px ;
}
.content h2
{
	text-align:center;
	color:#c71585;
	line-height:200px;
}
.content h5
{
	text-align:center;
	color:#fcdfff;
	letter-spacing:1px;
	margin:-75px 0px 0px ;
}
.content h4
{
	text-align:center;
	color:#f9b7ff;
	margin:10px 0px 0px ;
}
.box .img
{
	text-align:center;
	display:flex;
	margin:25px;
}
.box .img .icon1
{
	width:50px;
	height:50px;
	background-color:#c71585;
	border-radius:50px;
	margin:15px 4px 4px;
	overflow:hidden;
}
.box .img .icon1 .i
{
	margin:18px 0px 0px 0px;
}
.box:nth-child(2) .content h4
{
	text-align:center;
	line-height:70px;
	color:white;
}